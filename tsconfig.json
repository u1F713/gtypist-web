{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "noEmit": true,
    "types": ["vite/client"],
    "lib": ["DOM.Iterable"]
  },
  "exclude": ["node_modules"]
}
